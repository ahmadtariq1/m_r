{
  "version": 2,
  "builds": [
    {
      "src": "api/recommendations/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/recommendations",
      "dest": "/api/recommendations/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 